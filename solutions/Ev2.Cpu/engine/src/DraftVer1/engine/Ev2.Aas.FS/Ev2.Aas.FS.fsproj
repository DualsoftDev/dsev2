<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<!--
		<TargetFramework>netstandard2.0</TargetFramework>
		Project AasCore.Aas3_0 is not compatible with netstandard2.0 (.NETStandard,Version=v2.0). Project AasCore.Aas3_0 supports: net8.0 (.NETCoreApp,Version=v8.0)
		-->
		<TargetFramework>net9.0</TargetFramework>
		<LangVersion>9.0</LangVersion>
		<RootNamespace>Dual.Ev2.Aas</RootNamespace>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
	</PropertyGroup>

	<ItemGroup>
		<Content Include="AasSyntax.md" />
		<Compile Include="Core.Aas.fs" />
		<Compile Include="Core.To.Aas.fs" />
		<Compile Include="Core.To.Json.ViaCode.fs" />
		<Compile Include="Core.From.Aas.fs" />
	</ItemGroup>

	<ItemGroup>
		<!--<EmbeddedResource Remove="JsonData\**" />-->
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Ev2.Core.FS\Ev2.Core.FS.fsproj" />
	</ItemGroup>

	<!-- 해당 위치에 Aas3_0 이 check out 되어 있으면 그걸 쓰고, 없으면 nuget 참조 -->
	<PropertyGroup>
		<AasProjectPath>..\..\..\..\submodules\aas-core3.0-csharp\src\AasCore.Aas3_0\AasCore.Aas3_0.csproj</AasProjectPath>
	</PropertyGroup>

	<ItemGroup Condition="Exists('$(AasProjectPath)')">
		<ProjectReference Include="$(AasProjectPath)" />
	</ItemGroup>

	<ItemGroup Condition="!Exists('$(AasProjectPath)')">
		<PackageReference Include="AasCore.Aas3_0" Version="1.0.0" />
	</ItemGroup>


</Project>
