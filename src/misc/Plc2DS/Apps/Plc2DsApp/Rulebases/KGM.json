{
    "CsvFilterExpression": {
        "Case": "And",
        "Fields": [
          [
            {
              "Case": "Unit",
              "Fields": [
                {
                  "Name": "GlobalVariable, DirectVar 만 포함",
                  "PatternString": "^(GlobalVariable|DirectVar)$",
                  "Field": "Scope",
                  "Description": "GlobalVariable, DirectVar 만 포함"
                }
              ]
            },
            {
              "Case": "Unit",
              "Fields": [
                {
                  "Name": "IQM만 선택택",
                  "PatternString": "^%[I|Q|M]",
                  "Field": "Address",
                  "Description": ""
                }
              ]
            }
          ]
        ]
      },

    "TagPatternDiscards": [],
    "TagPatternReplaces": [
        {
            "Name": "Station, Robot 분리",
            "PatternString": "^R(?<station>\\d+)-(?<robot>\\d+)(?<sep>[\\b ])",
            "Replacement": "#${station}_RBT${robot}_",
            "Description": "'R303-1 ' => '#303_RBT1_'"
        },
        {
            "Name": "복합 Station 뒤에 명확히 '_' 추가",
            "PatternString": "^#(?<station1>\\d+)~(?<station2>\\d+)",
            "Replacement": "#${station1}<@~@>${station2}_",
            "Description": "'#137~301이송기 후진 록단 [YL]' => '#137~301_이송기 후진 록단 [YL]'"
        },
        {
            "Name": "공백을 '_' 로",
            "PatternString": "\\s+",
            "Replacement": "_",
            "Description": "공백을 '_' 로 변경"
        },
        {
            "Name": "[DI04] 제거",
            "PatternString": "[_\\s]\\[D[IOio]\\d+\\]",
            "Replacement": "",
            "Description": "'RBT1_[DO004]_그리퍼_차종유니트_LOCK_지령' => 'RBT1_그리퍼_차종유니트_LOCK_지령'"
        },
        {
            "Name": "꽁지 숫자 결합",
            "PatternString": "[_\\s](?<num>\\d+)$",
            "Replacement": "${num}",
            "Description": "'#303 S/G O100 PNL [LH] 제품감지 1' => '#303 S/G O100 PNL [LH] 제품감지1'"
        },
        {
            "Name": "강한결합 STN_RH_SAFETY_DOOR",
            "PatternString": "STN_(?<dir>LH|RH)_(?<word1>SAFETY)_(?<word2>DOOR)",
            "Replacement": "STN::${dir}::${word1}::${word2}",
            "Description": "CN, CHK, OK 이 분리되지 않도록 함"
        },
        {
            "Name": "강한결합 LOCK_POS",
            "PatternString": "[_\\s](LOCK_POS)$",
            "Replacement": "_LOCK::POS",
            "Description": ""
        },
        {
            "Name": "'_XXX [YYY]' 를 '_XXX' 으로 변경",
            "PatternString": "[_\\s](?<dan>[^_\\s]+)[_\\s]*\\[(?<op>[\\w]+)\\]$",
            "_Replacement": "_[${op}]_${dan}",
            "Replacement": "_${dan}",
            "Description": "'#137~301이송기 행거 풀림단 [GL]' => ''#137~301이송기 행거_풀림단'"
        },
        {
          "Name": "강한 결합 ' L/S '",
          "PatternString": "[_\\s](?<ls>[L|R]/S)[_\\s]",
          "Replacement": "::${ls}::",
          "Description": " 'R303-2 그리퍼 동작 1 전진 L/S 이상'  ==> 'R303-2 그리퍼 동작 1 전진::L/S::이상'"
        },
        {
          "Name": "강한 결합 '_XXX_이상' ",
          "PatternString": "[_\\s](?<err>TURN|LOCK|CLP|UNCLP|잠김|풀림)[_\\s]이상",
          "Replacement": "_${err}::이상",
          "Description": " 'R303-2 그리퍼 동작 1 전진_이상'  ==> 'R303-2 그리퍼 동작 1 전진::이상'"
        },
        {
          "Name": "강한 결합 'TRUN 지령 LH' ",
          "PatternString": "[_\\s](?<cmd1>TURN|LOCK)[_\\s](?<cmd2>지령|확인단)[_\\s](?<cmd3>LH|RH)",
          "Replacement": "_${cmd1}::${cmd2}::${cmd3}",
          "Description": " 'R303-1 [Do002] 그리퍼 차종유니트 LOCK 지령 LH'  ==> 'R303-1 [Do002] 그리퍼 차종유니트_LOCK::지령::LH'"
        }
    ],
    "TagPatternFDAs": [],

    "FlowPatternReplaces": [
        {
            "Name": "임시기호 복원",
            "PatternString": "<@(?<symbol>[^@]+)@>",
            "Replacement": "${symbol}",
            "Description": "임시로 변경한 기호를 원복"
        }
    ],
    "DevicePatternReplaces": [],
    "_ActionPatternReplaces": []
  }

