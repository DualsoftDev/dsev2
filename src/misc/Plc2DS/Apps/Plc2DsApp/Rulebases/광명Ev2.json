{
  "_CsvFilterPatterns": [],

  "TagPatternDiscards": [
    {
      "Name": "'_' 갯수 미달",
      "PatternString": "^[^_]+(_[^_]+)?$",
      "Description": "'_'구분자가 없거나, 하나만 존재하는 항목"
    },
    {
      "Name": "'_CN_숫자' 로 마감",
      "PatternString": "_CN_\\d+$",
      "Description": "'_CN_' 와 숫자로 끝나는 항목"
    },
    {
      "Name": "'_ECHO_숫자' 로 마감",
      "PatternString": "_ECHO_\\d+$",
      "Description": "'_ECHO_' 와 숫자로 끝나는 항목"
    },
    {
      "Name": "'_(SELECT|CTYPE)_숫자' 로 마감",
      "PatternString": "_(SELECT|CTYPE)_\\d+$",
      "Description": "'_SELECT_' 혹은 '_CTYPE_' 와 숫자로 끝나는 항목"
    },
    {
      "Name": "'_BT_' 포함",
      "PatternString": "_BT(\\d)*_",
      "Description": "'_BT_' 혹은 '_BT숫자_' 포함하는 항목"
    },
    {
      "Name": "'_OPT_' 포함",
      "PatternString": "_OPT(\\d)*_",
      "Description": "'_OPT_' 혹은 '_OPT숫자_' 포함하는 항목"
    },
    {
      "Name": "'_BOLTING_ERR_' 포함",
      "PatternString": "_BOLTING_ERR(\\d)*_",
      "Description": "'_BOLTING_ERR_' 혹은 '_BOLTING_ERR숫자_' 포함하는 항목"
    },
    {
      "Name": "'_COMM_ERR' 로 마감",
      "PatternString": "_COMM_ERR$",
      "Description": "'_COMM_ERR' 로 끝나는 항목"
    },
    {
      "Name": "'_CARR_TYPE_[A-Z]' 로 마감",
      "PatternString": "_C(AR(R)?)?(_)?TYPE_[A-Z]$",
      "Description": "CARR_TYPE_, CTYPE_ 뒤에 alphabet 으로 끝나는 항목. e.g '_CTYPE_E'"
    },
    {
      "Name": "'_CARR_NO_숫자' 로 마감",
      "PatternString": "_CARR_NO_\\d+$",
      "Description": "CARR_NO_' 뒤에 숫자로 끝나는 항목. e.g '_CARR_NO_16'"
    },
    {
      "Name": "'_SERVO' 포함",
      "PatternString": "_SERVO\\.",
      "Description": "'_SERVO.' 을 포함하는 항목"
    },
    {
      "Name": "'_SRV_' 포함",
      "PatternString": "_(SRV\\d*|SERVO\\d*)_",
      "Description": "'_SRV_' 또는 '_SERVO_' 을 포함하는 항목"
    },
    {
      "Name": "'_BOLTING_ERR_' 포함",
      "PatternString": "__BOLTING_ERR__",
      "Description": "'_BOLTING_ERR_' 을 포함하는 항목"
    },
    {
      "Name": "'_MANUAL_INT' 포함",
      "PatternString": "_(MANUAL|MUTUAL)_INT\\d*",
      "Description": "'_MANUAL_INT' 을 포함하는 항목"
    }
  ],
  "TagPatternReplaces": [
    {
      "Name": "공백을 '_' 로",
      "PatternString": "[\\s]+",
      "Replacement": "_",
      "Description": "공백을 '_' 로 대체"
    },
    {
      "Name": "Station, Device 분리",
      "PatternString": "^(?<station>S\\d+)(?<device>[a-zA-Z]+\\d+)_.*$",
      "Replacement": "${station}_${device}_",
      "Description": "S302RBT4_ => S302_RB4_"
    },
    {
      "Name": "단어/숫자만",
      "PatternString": "_(?<word>[A-Za-z]+)[_/](?<number>\\d+)$",
      "Replacement": "_${word}${number}",
      "Description": "e.g '_A/1' 혹은 '_A_1' => '_A1'"
    },
    {
      "Name": "강한결합 CN_CHK_OK",
      "PatternString": "_(?<word1>CN)_(?<word2>CHK|CHECK)_(?<word3>OK|USING|REQUEST|USE|UNUSE)$",
      "Replacement": "_${word1}::${word2}::${word3}",
      "Description": "CN, CHK, OK 이 분리되지 않도록 함"
    },
    {
      "Name": "강한결합 LOADING_COMPL",
      "PatternString": "_(?<word1>(UN)?LOADING)_(?<word2>(COMPL|OK))$",
      "Replacement": "_${word1}::${word2}",
      "Description": "LOADING 과 COMPL 이 분리되지 않도록 함"
    },
    {
      "Name": "강한결합 COUNT_RESET",
      "PatternString": "_(?<word1>COUNT)_(?<word2>(RESET|WARNING))$",
      "Replacement": "_${word1}::${word2}",
      "Description": "COUNT 과 RESET 이 분리되지 않도록 함"
    },
    {
      "Name": "강한결합 TYPE_CHECK",
      "PatternString": "_(?<word1>(TYPE|LAST))_(?<word2>(CHK|CHECK)\\d*)$",
      "Replacement": "_${word1}::${word2}",
      "Description": "TYPE 과 CHK 이 분리되지 않도록 함"
    }
  ],
  "TagPatternFDAs": [],

  "FlowPatternReplaces": [],
  "DevicePatternReplaces": [],
  "_ActionPatternReplaces": []
}

