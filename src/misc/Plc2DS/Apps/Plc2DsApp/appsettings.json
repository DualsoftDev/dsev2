  "TagPatternDiscards": [
    {
      "Name": "'_' 갯수 미달",
      "PatternString": "^[^_]+(_[^_]+)?$",
      "Description": "'_'구분자가 없거나, 하나만 존재하는 항목"
    },
    {
      "Name": "'_' 로 시작",
      "PatternString": "^_",
      "Description": "'_' 로 시작하는 항목"
    },
    {
      "Name": "'[숫자]' indexing",
      "PatternString": "\\[\\d+\\]$",
      "Description": "'[' 와 ']' 사이에 숫자로 끝나는 항목"
    },
    {
      "Name": "'_CN_숫자' 로 마감",
      "PatternString": "_CN_\\d+$",
      "Description": "'_CN_' 와 숫자로 끝나는 항목"
    },
    {
      "Name": "'_ECHO_숫자' 로 마감",
      "PatternString": "_ECHO_\\d+$",
      "Description": "'_ECHO_' 와 숫자로 끝나는 항목"
    },
    {
      "Name": "'_(SELECT|CTYPE)_숫자' 로 마감",
      "PatternString": "_(SELECT|CTYPE)_\\d+$",
      "Description": "'_SELECT_' 혹은 '_CTYPE_' 와 숫자로 끝나는 항목"
    },
    {
      "Name": "'_BT_' 포함",
      "PatternString": "_BT(\\d)*_",
      "Description": "'_BT_' 혹은 '_BT숫자_' 포함하는 항목"
    },
    {
      "Name": "'_OPT_' 포함",
      "PatternString": "_OPT(\\d)*_",
      "Description": "'_OPT_' 혹은 '_OPT숫자_' 포함하는 항목"
    },
    {
      "Name": "'_BOLTING_ERR_' 포함",
      "PatternString": "_BOLTING_ERR(\\d)*_",
      "Description": "'_BOLTING_ERR_' 혹은 '_BOLTING_ERR숫자_' 포함하는 항목"
    },
    {
      "Name": "'_COMM_ERR' 로 마감",
      "PatternString": "_COMM_ERR$",
      "Description": "'_COMM_ERR' 로 끝나는 항목"
    },
    {
      "Name": "'_CARR_TYPE_[A-Z]' 로 마감",
      "PatternString": "_C(AR(R)?)?(_)?TYPE_[A-Z]$",
      "Description": "CARR_TYPE_, CTYPE_ 뒤에 alphabet 으로 끝나는 항목. e.g '_CTYPE_E'"
    },
    {
      "Name": "'_CARR_NO_숫자' 로 마감",
      "PatternString": "_CARR_NO_\\d+$",
      "Description": "CARR_NO_' 뒤에 숫자로 끝나는 항목. e.g '_CARR_NO_16'"
    },
    {
      "Name": "'_SERVO' 포함",
      "PatternString": "_SERVO\\.",
      "Description": "'_SERVO.' 을 포함하는 항목"
    },
    {
      "Name": "'_SRV_' 포함",
      "PatternString": "_(SRV\\d*|SERVO\\d*)_",
      "Description": "'_SRV_' 또는 '_SERVO_' 을 포함하는 항목"
    },
    {
      "Name": "'_BOLTING_ERR_' 포함",
      "PatternString": "__BOLTING_ERR__",
      "Description": "'_BOLTING_ERR_' 을 포함하는 항목"
    },
    {
      "Name": "'_MANUAL_INT' 포함",
      "PatternString": "_(MANUAL|MUTUAL)_INT\\d*",
      "Description": "'_MANUAL_INT' 을 포함하는 항목"
    }
  ],
  "TagPatternReplaces": [
    {
      "Name": "중복 '_' 제거",
      "PatternString": "_+",
      "Replacement": "_",
      "Description": "연속된 언더스코어를 하나로 줄임"
    },
    {
      "Name": "점을 '_' 로",
      "PatternString": "\\.",
      "Replacement": "_",
      "Description": "'.' 을 '_' 로 변경"
    }
  ],
  "TagPatternFDAs": [
    {
      "Name": "ROBOT 패턴",
      "PatternString": "^(?<flow>[^_]+)_([IQM]_)?(?<device>(RB|RBT|ROBOT)\\d+)_(?<action>.*)$",
      "Description": "e.g S301_I_RB2_1ST_WORK_COMP => {S301, RB2, 1ST_WORK_COMP} 로 분리"
    },
    {
      "Name": "LATch 패턴",
      "PatternString": "^(?<flow>[^_]+)_([IQM]_)?(?<device>(.*)_LAT)_(?<action>.*)$",
      "Description": "_LAT_ 기준으로 분리"
    },
    {
      "Name": "AUX/ERR 패턴",
      "PatternString": "^(?<flow>[^_]+)_(?<device>(.*))_(?<action>(ADV|RET)_(AUX|ERR))$",
      "Description": "e.g S301_I_RB2_1ST_WORK_COMP => {S301, RB2, 1ST_WORK_COMP} 로 분리"
    },
    {
      "Name": "LS/SOL 패턴",
      "PatternString": "^(?<flow>[^_]+)_((LS|SOL|I|Q)_)?(?<device>(.*))_(?<action>[^_]+)$",
      "Description": "e.g S301_I_RB2_1ST_WORK_COMP => {S301, RB2, 1ST_WORK_COMP} 로 분리"
    },
    {
      "Name": "일반 패턴",
      "PatternString": "^(?<flow>[^_]+)_(?<device>.*)_(?<action>[^_]+)$",
      "Description": "e.g S301_I_RB2_1ST_WORK_COMP => {S301, RB2, 1ST_WORK_COMP} 로 분리"
    }
  ],
  "FlowPatternDiscards": [],
