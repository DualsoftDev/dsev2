<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net8.0-windows</TargetFramework>
		<Platform Condition=" '$(Platform)' == '' ">x86</Platform>
		<LangVersion>12.0</LangVersion>
		<OutputType>WinExe</OutputType>
		<SatelliteResourceLanguages>en;ko</SatelliteResourceLanguages>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<UseWindowsForms>true</UseWindowsForms>
		<ImportWindowsDesktopTargets>true</ImportWindowsDesktopTargets>
		<NoWarn>$(NoWarn);CA1416</NoWarn>
	</PropertyGroup>


	<ItemGroup>
		<EmbeddedResource Include="Properties\licenses.licx" />
	</ItemGroup>
	<ItemGroup>
		<Content Include="Rulebases\**\*.json">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
			<CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
			<Publish>true</Publish>
		</Content>
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\..\..\..\..\submodules\nuget\Common\Dual.Common.Base.CS\Dual.Common.Base.CS.csproj" />
		<ProjectReference Include="..\..\..\..\..\submodules\nuget\Common\Dual.Common.Base.FS\Dual.Common.Base.FS.fsproj" />
		<ProjectReference Include="..\..\..\..\..\submodules\nuget\Common\Dual.Common.Core\Dual.Common.Core.csproj" />
		<ProjectReference Include="..\..\..\..\..\submodules\nuget\Windows\Dual.Common.Winform.DevX\Dual.Common.Winform.DevX.24.1.7.csproj" />
		<ProjectReference Include="..\..\..\..\..\submodules\nuget\Windows\Dual.Common.Winform\Dual.Common.Winform.csproj" />
		<ProjectReference Include="..\..\Dual.Plc2DS\Dual.Plc2DS.fsproj" />
	</ItemGroup>
	<ItemGroup>
		<Content Include="appsettings.json">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
			<Publish>true</Publish>
		</Content>
		<Content Include="CheatKey.txt" />
		<Content Include="log4net.config">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="DevExpress.Win" Version="24.1.7" />
		<PackageReference Include="System.Configuration" />
		<PackageReference Include="log4net" Version="2.0.17" />
		<PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
		<PackageReference Include="System.Data.DataSetExtensions" Version="4.5.0" />
		<PackageReference Include="UTF.Unknown" Version="2.5.1" />
	</ItemGroup>
	<ItemGroup>
		<Compile Remove="Unused\AppSettings.cs" />
	</ItemGroup>
</Project>
