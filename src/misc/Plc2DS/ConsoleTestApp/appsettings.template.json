{
  "_NameSeparators": "_",
  "_SplitOnCamelCase": false,

  "RegexPattern": "^(?<flow>[^_]+)_(?<device>.+)_(?<action>[^_]+)$",

  "Dialects": [
    [ "ADV", "ADVANCE" ],
    [ "CLP", "CLAMP" ],
    [ "CYL", "CYLINDER" ],
    [ "LOAD", "LOADG" ],
    [ "RET", "RETURN", "RTN" ],
    [ "RBT", "ROBOT", "RB" ],
    [ "ROT", "ROTATE" ],
    [ "SHT", "SHUTTLE" ],
    [ "STN", "STATION" ],
    [ "UNCLP", "UNCLAMP" ]
  ],

  "__이하 _Actions 및 States, MutualResetTuples 등 모든 항목": "표준어로만 기술할 것",

  "Actions": [ "ADV", "CHECK", "HOME", "LOAD", "OFF", "ON", "PRESS", "RET", "ROTATE", "SELECT", "SWING", "UNSWING" ],

  "__DeviceNames": [ "CAM", "CARRIER", "CONNECTOR", "CYL", "GATE", "GUN", "LAMP", "LOCK", "LS", "MOTOR", "PART", "PIN", "RBT", "SHT", "SLIDE", "SOL", "VALVE" ],
  "SpecialFlows": [ "MY_SPECIAL_STN", "ANOTHER_SPECIAL_STN" ],
  "SpecialActions": [ "MY_SPECIAL_ACTION", "ANOTHER_SPECIAL_ACTION" ],
  
  "Discards": ["I", "Q", "LS"],


  "MutualResetTuples": [
    [ "ADV", "RET" ],
    [ "LATCH", "UNLATCH" ],
    [ "MATCHING", "UNMATCH" ],
    [ "ON", "OFF" ],
    [ "OPEN", "CLOSE" ],
    [ "SWING", "UNSWING" ],
    [ "UP", "DOWN" ]
  ],

  "States": [ "CONDITION", "ERR", "NO", "OK", "RUNNING", "STOP", "WAIT" ],
  "AddOn": {
    "AB": {
      "Actions": [ "ABORT", "PAUSE", "RESUME" ],
      "States": [ "ABORTED", "PAUSED" ]
    },
    "MX": {
    }
  },
  "Override": {
    "AB": {
      "Actions": [ "ABORT", "PAUSE", "RESUME" ],
      "States": [ "ABORTED", "PAUSED" ]
    },
    "MX": {
      "NameSeparators": [ "_", ";", " " ],
      "States": [ "MOVED" ]
    }
  }
}

