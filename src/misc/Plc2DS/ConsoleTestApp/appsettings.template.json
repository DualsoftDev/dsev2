{
  "_NameSeparators": "_",
  "_SplitOnCamelCase": false,
  "_PreferPrefixModifier": false,


  "Dialects": [
    [ "ADV", "ADVANCE" ],
    [ "CLP", "CLAMP" ],
    [ "CYL", "CYLINDER" ],
    [ "LOAD", "LOADG" ],
    [ "RET", "RETURN", "RTN" ],
    [ "RBT", "ROBOT", "RB" ],
    [ "ROT", "ROTATE" ],
    [ "SHT", "SHUTTLE" ],
    [ "STN", "STATION" ],
    [ "UNCLP", "UNCLAMP" ]
  ],

  "_PositionHints": "항목 (Flow, Device, Action, State) 등의 '_' 기준 가능한 위치를 백분율로 나타낸 것. Flow 가 앞에 오고 Action 이 뒤에 온다면 다음 처럼",
  "PositionHints": {
    "Flow"  : { "Min": 0,  "Max": 40 },
    "Device": { "Min": 20, "Max": 80 },
    "Action": { "Min": 50, "Max": 100 },
    "State" : { "Min": 70, "Max": 100 }
  },

  "__이하 _Actions 및 States, MutualResetTuples 등 모든 항목": "표준어로만 기술할 것",

  "Actions": [ "ADV", "CHECK", "HOME", "LOAD", "OFF", "ON", "PRESS", "RET", "ROTATE", "SELECT", "SWING", "UNSWING" ],

  "DeviceNames": [ "CAM", "CARRIER", "CONNECTOR", "CYL", "GATE", "GUN", "LAMP", "LOCK", "LS", "MOTOR", "PART", "PIN", "RBT", "SHT", "SLIDE", "SOL", "VALVE" ],
  "_DeviceNameFragments": "Device 이름의 일부로 사용되는 Fragment",
  "DeviceNameFragments": [ "CYL" ],

  "FlowNames": [ "STN" ],
  "_FlowNameFragments": "Flow 이름의 일부로 사용되는 Fragment",
  "FlowNameFragments": [ "STN" ],

  "Discards": ["I"; "Q"],

  "_PrefixModifiers": "Flow / Device 의 접두사로 사용되는 Modifier",
  "PrefixModifiers": [
  ],
  "_PostfixModifiers": "Flow / Device 의 접미사로 사용되는 Modifier",
  "PostfixModifiers": [
  ],
  "_Modifiers": "대상이나 위치 고려하지 않은 Modifier",
  "Modifiers": [
    "A", "ALL", "AUX", "B", "C", "D",
    "EMERGENCY", "HIGH", "LOW", "MEMO", "N",
    "SIDE", "SPEED", "1ST", "2ND", "3RD", "4TH",
    "2UNIT", "3UNIT", "4UNIT", "3DR", "4DR", "5DR",
    "UNIT"
  ],


  "MutualResetTuples": [
    [ "ADV", "RET" ],
    [ "LATCH", "UNLATCH" ],
    [ "MATCHING", "UNMATCH" ],
    [ "ON", "OFF" ],
    [ "OPEN", "CLOSE" ],
    [ "SWING", "UNSWING" ],
    [ "UP", "DOWN" ]
  ],

  "States": [ "CONDITION", "ERR", "NO", "OK", "RUNNING", "STOP", "WAIT" ],
  "AddOn": {
    "AB": {
      "Actions": [ "ABORT", "PAUSE", "RESUME" ],
      "States": [ "ABORTED", "PAUSED" ]
    },
    "MX": {
    }
  },
  "Override": {
    "AB": {
      "Actions": [ "ABORT", "PAUSE", "RESUME" ],
      "States": [ "ABORTED", "PAUSED" ]
    },
    "MX": {
      "NameSeparators": [ "_", ";", " " ],
      "States": [ "MOVED" ]
    }
  }
}

